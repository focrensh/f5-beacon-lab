{
    "class": "Telemetry",
    "Poller": {
      "class": "Telemetry_System_Poller",
      "interval": 60,
      "enable": true,
      "actions": [
        {
          "enable": true,
          "includeData": {},
          "locations": {
            "system": true,
            "virtualServers": true,
            "httpProfiles": true,
            "clientSslProfiles": true,
            "serverSslProfiles": true
          }
        }
      ]
    },
    "My_Listener": {
      "class": "Telemetry_Listener",
      "port": 6514,
      "actions": [
        {
            "enable": true,
            "excludeData": {},
            "ifAllMatch": {
                "Entity": "^(?!VipStat|OffboxAll).*$"
            },
            "locations": {
                ".*": {}
            }
        }
    ]
    },
    "Beacon_Consumer": {
      "class": "Telemetry_Consumer",
      "type": "Generic_HTTP",
      "host": "ingestion.ovr.prd.f5aas.com",
      "protocol": "https",
      "port": 50443,
      "path": "/beacon/v1/ingest-telemetry-streaming",
      "method": "POST",
      "enable": true,
      "trace": false,
      "headers": [
        {
          "name": "grpc-metadata-x-f5-ingestion-token",
          "value": "`>@/passphrase`"
        }
      ],
      "passphrase": {
        "cipherText": "{{ beacon_token }}"
      }
    },
    "schemaVersion": "1.0.0"
  }
  